        <script type="text/javascript">
            var count = 4; // Timer
            var redirect = "https://premiumgrealty.com/"; // Target URL


            $(document).ready(function () {
                countDown();
            });

            function countDown() {
                var timer = document.getElementById("timer"); // Timer ID
                if (count > 0) {
                    count--;
                    timer.innerHTML = "You will be redirected to our web in " + count + " seconds."; // Timer Message
                    setTimeout("countDown()", 1000);
                } else {
                    window.location.href = redirect;
                }
            }
        </script>
    
