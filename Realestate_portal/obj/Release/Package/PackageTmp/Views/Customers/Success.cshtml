
@{
    ViewBag.Title = "Success";
    Layout = "~/Views/Shared/_WizardForm.cshtml";
}

<div class="container-fluid full-height">
    <div class="row row-height">
        <a href="https://premiumgrealty.com/" id="logo"><img src="~/Content/pgrlogos/PGRLogo.png" alt="" style="width:189px;"></a>
        <div id="success">

            <div class="icon icon--order-success svg">
                <svg xmlns="http://www.w3.org/2000/svg" width="72px" height="72px">
                    <g fill="none" stroke="#8EC343" stroke-width="2">
                        <circle cx="36" cy="36" r="35" style="stroke-dasharray:240px, 240px; stroke-dashoffset: 480px;"></circle>
                        <path d="M17.417,37.778l9.93,9.909l25.444-25.393" style="stroke-dasharray:50px, 50px; stroke-dashoffset: 0px;"></path>
                    </g>
                </svg>
            </div>
            <h4><span></span>Your message has been successfully sent. </h4><br />
            <h4><span>We will reply in less than 24 hours.</span></h4>
            <div class="row" style="margin-top:30px">
                <p id="timer">
                </p>
            </div>

        </div>

    </div>
    <!-- /row-->
</div>
