    <link rel="stylesheet" href="~/Content/assets/css/owl-carousel/owl.theme.min.css">
    <!-- Tweaks for older IEs-->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
