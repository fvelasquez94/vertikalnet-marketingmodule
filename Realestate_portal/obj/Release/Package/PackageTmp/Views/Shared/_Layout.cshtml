<!--NOTIFICACIONES GLOBALES-->

@if (TempData["error"] != null)
{
    <script type="text/javascript">
        window.onload = function () {

            new Noty({
                type: 'error',
                layout: 'topRight',
                text: '@(TempData["error"])',
                progressBar: true,
                timeout: 2500
            }).show()
        };
    </script>
}


@if (TempData["advertencia"] != null)
{
    <script type="text/javascript">

        $(document).ready(function () {

            new Noty({
                type: 'warning',
                layout: 'topRight',
                text: '@(TempData["advertencia"])',
                progressBar: true,
                timeout: 2500
            
            }).show()

        });
    </script>
}


@if (TempData["exito"] != null)
{
    <script type="text/javascript">

        $(document).ready(function () {

            $('#status').change(function () {
                console.log(this.value);
            });

            new Noty({
                type: 'success',
                layout: 'topRight',
                text: '@(TempData["exito"])',
                progressBar: true,
                timeout: 2500
            }).show()
        });


       
    </script>
}

    <script>
    function CambiarBroker(broker) {


        var url = '@Url.Action("Dashboard","Portal", new { broker="IDCOMPANY"})';

        url = url.replace("IDCOMPANY", broker);

        window.location.href = url;
    }
    </script>